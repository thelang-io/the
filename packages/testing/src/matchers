fn MATCH_REF_BOOL (a: ref bool, b: ref bool) {
  if a != b {
    FAIL_TEST(
      "Expected equality of these values:" + os_EOL +
      "  " + a.str() + os_EOL +
      "  " + b.str()
    )
  }
}

fn MATCH_REF_BYTE (a: ref byte, b: ref byte) {
  if a != b {

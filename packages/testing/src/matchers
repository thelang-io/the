fn MATCH_OPTIONAL_BOOL (a: bool?, b: bool?) {
  if a != b {
    FAIL_TEST(
      "Expected equality of these values:" + os_EOL +
      "  " + a.str() + os_EOL +
      "  " + b.str()
    )
  }
}


  if a != b {
    FAIL_TEST(
      "Expected equality of these values:" + os_EOL +
      "  " + a.str() + os_EOL +
      "  " + b.str()
    )
  }
}

fn MATCH_REF_FLOAT (a: ref float, b: ref float) {
  if a != b {
    FAIL_TEST(
      "Expected equality of these values:" + os_EOL +
      "  " + a.str() + os_EOL +
      "  " + b.str()
    )
  }
}

fn MATCH_REF_INT (a: ref int, b: ref int) {
  if a != b {
    FAIL_TEST(
      "Expected equality of these values:" + os_EOL +
      "  " + a.str() + os_EOL +
      "  " + b.str()
    )
  }
